{
  "$schema": "http://json-schema.org/draft-04/schema",

  "type": "object",

  "properties": {
    "id": { "type": "integer" },
    "streetAddress": { "type": "string" },
    "latitude": { "type": "number" },
    "longitude": { "type": "number" },
    "openHours": {
      "type": "array",
      "items": { "$ref": "openingHour" }
    }
  },
  "required": ["id", "streetAddress", "latitude", "longitude", "openHours"]
}
