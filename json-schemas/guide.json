{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "definitions": {
        "Beacon": {
            "properties": {
                "distance": {
                    "type": "number"
                },
                "id": {
                    "type": "string"
                },
                "location": {
                    "$ref": "#/definitions/Position"
                },
                "nid": {
                    "type": "string"
                }
            },
            "required": [
                "distance",
                "id",
                "location",
                "nid"
            ],
            "type": "object"
        },
        "ContentObject": {
            "properties": {
                "audio": {
                    "$ref": "#/definitions/MediaContent"
                },
                "beacon": {
                    "$ref": "#/definitions/Beacon"
                },
                "description": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "images": {
                    "items": {
                        "$ref": "#/definitions/ImageUrls"
                    },
                    "type": "array"
                },
                "links": {
                    "items": {
                        "$ref": "#/definitions/Link"
                    },
                    "type": "array"
                },
                "order": {
                    "type": "number"
                },
                "postStatus": {
                    "$ref": "#/definitions/PostStatus"
                },
                "searchableId": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "video": {
                    "$ref": "#/definitions/MediaContent"
                }
            },
            "required": [
                "id",
                "images",
                "order",
                "postStatus",
                "searchableId",
                "title"
            ],
            "type": "object"
        },
        "ContentType": {
            "enum": [
                "audio",
                "video"
            ],
            "type": "string"
        },
        "GuideType": {
            "enum": [
                "guide",
                "trail"
            ],
            "type": "string"
        },
        "ImageUrls": {
            "properties": {
                "large": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Url"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "medium": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Url"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "thumbnail": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/Url"
                        },
                        {
                            "type": "null"
                        }
                    ]
                }
            },
            "type": "object"
        },
        "Link": {
            "properties": {
                "title": {
                    "type": "string"
                },
                "type": {
                    "enum": [
                        "facebook",
                        "instagram",
                        "spotify",
                        "twitter",
                        "vimeo",
                        "web",
                        "youtube"
                    ],
                    "type": "string"
                },
                "url": {
                    "$ref": "#/definitions/Url"
                }
            },
            "required": [
                "url"
            ],
            "type": "object"
        },
        "MediaContent": {
            "properties": {
                "contentType": {
                    "$ref": "#/definitions/ContentType"
                },
                "created": {
                    "format": "date-time",
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "modified": {
                    "format": "date-time",
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "url": {
                    "$ref": "#/definitions/Url"
                }
            },
            "required": [
                "contentType",
                "created",
                "description",
                "id",
                "modified",
                "title",
                "url"
            ],
            "type": "object"
        },
        "ParsedUrlQuery": {
            "additionalProperties": {
                "anyOf": [
                    {
                        "items": {
                            "type": "string"
                        },
                        "type": "array"
                    },
                    {
                        "type": "string"
                    }
                ]
            },
            "type": "object"
        },
        "Position": {
            "properties": {
                "latitude": {
                    "type": "number"
                },
                "longitude": {
                    "type": "number"
                }
            },
            "required": [
                "latitude",
                "longitude"
            ],
            "type": "object"
        },
        "PostStatus": {
            "enum": [
                "draft",
                "publish"
            ],
            "type": "string"
        },
        "Url": {
            "properties": {
                "auth": {
                    "type": "string"
                },
                "hash": {
                    "type": "string"
                },
                "host": {
                    "type": "string"
                },
                "hostname": {
                    "type": "string"
                },
                "href": {
                    "type": "string"
                },
                "path": {
                    "type": "string"
                },
                "pathname": {
                    "type": "string"
                },
                "port": {
                    "type": "string"
                },
                "protocol": {
                    "type": "string"
                },
                "query": {
                    "anyOf": [
                        {
                            "$ref": "#/definitions/ParsedUrlQuery"
                        },
                        {
                            "type": [
                                "null",
                                "string"
                            ]
                        }
                    ]
                },
                "search": {
                    "type": "string"
                },
                "slashes": {
                    "type": "boolean"
                }
            },
            "type": "object"
        }
    },
    "properties": {
        "childFriendly": {
            "type": "boolean"
        },
        "contentObjects": {
            "items": {
                "$ref": "#/definitions/ContentObject"
            },
            "type": "array"
        },
        "dateEnd": {
            "format": "date-time",
            "type": "string"
        },
        "dateStart": {
            "format": "date-time",
            "type": "string"
        },
        "description": {
            "type": [
                "null",
                "string"
            ]
        },
        "guideGroupId": {
            "type": "number"
        },
        "guideType": {
            "$ref": "#/definitions/GuideType"
        },
        "id": {
            "type": "number"
        },
        "images": {
            "$ref": "#/definitions/ImageUrls"
        },
        "name": {
            "type": "string"
        },
        "postStatus": {
            "$ref": "#/definitions/PostStatus"
        },
        "slug": {
            "type": "string"
        },
        "tagline": {
            "type": [
                "null",
                "string"
            ]
        }
    },
    "required": [
        "childFriendly",
        "contentObjects",
        "guideGroupId",
        "guideType",
        "id",
        "images",
        "name",
        "postStatus",
        "slug"
    ],
    "type": "object"
}

