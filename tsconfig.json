{
  "compileOnSave": true,
  "compilerOptions": {
    "strictNullChecks": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "baseUrl": "."
  },
  "include": ["src/**/*"]
}
