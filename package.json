{
  "name": "app-helsingborg-nodejs",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "8.9.0",
    "npm": "5.5.1"
  },
  "scripts": {
    "start": "DEBUG=app,warn node dist/app.js",
    "tsc": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "DEBUG=app,warn nodemon --inspect dist/app.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"blue.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build": "npm run tsc",
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "tslint": "tslint --project ."
  },
  "dependencies": {
    "ajv": "6.4.0",
    "debug": "2.6.9",
    "express": "4.16.0",
    "express-validator": "5.1.2",
    "morgan": "1.9.0",
    "node-fetch": "2.1.2",
    "typescript": "2.8.3"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "4.11.1",
    "@types/morgan": "1.7.35",
    "@types/node": "8.9.0",
    "@types/node-fetch": "1.6.9",
    "concurrently": "3.5.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.10.0",
    "nodemon": "1.17.3",
    "tslint": "5.9.1",
    "typescript-json-schema": "^0.23.0"
  }
}
